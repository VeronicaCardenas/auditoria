                        }break;
                        case "5" :{//case 'DIARIA':{
                            tasaPeriodica=tasa/360;
                            $("#tasamensual").html('<span id="" class="label label-info">La tasa equivalente asociada al período es del '+tasaPeriodica.toFixed(4)+' %</span>')
                            $("#tasamensual").show();
                        }break;
                        case "6" :{//case 'ANUAL':{
                            tasaPeriodica=tasa/1;
                            $("#tasamensual").html('<span id="" class="label label-info">La tasa equivalente asociada al período es del '+tasaPeriodica.toFixed(4)+' %</span>')
