    <script>

        $(document).ready(function () {
            $(document).on("click","#aportes",function(){

                var mes = $('#mes').val();
                var ano = $('#ano').val();
                if( mes==0 || ano==""){
                    BootstrapDialog.show({
                        type: BootstrapDialog.TYPE_DEFAULT,
                        size: BootstrapDialog.SIZE_SMALL,
                        title: 'Error al visualizar el resumen',
                        message: 'Debe seleccionar el mes y el año.'
                    });
                }else{
                    $('#aportes').attr("href","{{ path('excedentes',{'mes':'','ano':''}) }}" + '/' + mes + '/'+ ano);
                }
            });

            $(document).on("click","#exportExcedente",function(){

                var mes = $('#mes').val();
                var ano = $('#ano').val();
                if( mes==0 || ano==""){
                    BootstrapDialog.show({
                        type: BootstrapDialog.TYPE_DEFAULT,
                        size: BootstrapDialog.SIZE_SMALL,
                        title: 'Error al visualizar el resumen',
                        message: 'Debe seleccionar el mes y el año.'
                    });
                }else{
                    $('#exportExcedente').attr("href","{{ path('imprimirExcedentes',{'mes':'','ano':''}) }}" + '/' + mes + '/'+ ano);
                }
            });

            $("#verexcedentes").addClass("active");
            $("#Contabilidad").addClass("active");
        });

    </script>
